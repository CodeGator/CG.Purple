{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DAL": {
    "DropDatabaseOnStartup": false,
    "MigrateDatabaseOnStartup": false,
    "ConnectionString": ""
  },
  "BLL": {
    "FileTypes": {
      "DefaultCacheDuration": "00.00:30:00"
    },
    "MailMessages": {
      "DefaultCacheDuration": "00.00:00:30"
    },
    "TextMessages": {
      "DefaultCacheDuration": "00.00:00:30"
    }
  },
  "Seeding": {
    "SeedOnStartup": false,
    "Force": false,
    "FileNames": [
      "Seeding/mimeTypes.json",
      "Seeding/parameterTypes.json",
      "Seeding/propertyTypes.json",
      "Seeding/providerTypes.json",
      "Seeding/providerParameters.json",
      "Seeding/mailMessages.json",
      "Seeding/textMessages.json",
      "Seeding/providerLogs.json"
    ]
  },
  "DataProtection": {
    "DisableAutomaticKeyGeneration": false,
    "AzureKeyStorage": {
      "ConnectionString": "",
      "ContainerName": "",
      "BlobName": ""
    }
  }
}
